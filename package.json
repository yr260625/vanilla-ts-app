{
  "name": "vanilla-ts-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run api\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
