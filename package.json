    "@vitest/coverage-v8": "^0.34.2",
    "concurrently": "^8.2.0",
    "eslint": "^8.44.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "happy-dom": "^10.11.0",
    "json-server": "^0.17.3",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.2"
  }
